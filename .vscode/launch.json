    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch App",
            "request": "launch",
            "type": "pwa-node",
            "program": "index.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--inspect-brk",
                "--allow-all",
                "--unstable"
            ],
            "outputCapture": "std",
            "attachSimplePort": 9229
        },
        {
            "name": "Tests",
            "request": "launch",
            "type": "pwa-node",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "test",
                "--inspect-brk",
                "--allow-all",
                "--unstable"
            ],
            "outputCapture": "std",
            "attachSimplePort": 9229
        },
        {
            "name": "Tests (filtered)",
            "request": "launch",
            "type": "pwa-node",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "test",
                "--inspect-brk",
                "--allow-all",
                "--unstable",
                "--filter",
                "alternate scenario test"
            ],
            "outputCapture": "std",
            "attachSimplePort": 9229
        },
        {
            "name": "Tests (filtered 2)",
            "request": "launch",
            "type": "pwa-node",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "test",
                "--inspect-brk",
                "--allow-all",
                "--unstable",
                "--filter",
                "same point test"
            ],
            "outputCapture": "std",
            "attachSimplePort": 9229
        },
        {
            "name": "Tests (readFileContents)",
            "request": "launch",
            "type": "pwa-node",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "test",
                "--inspect-brk",
                "--allow-all",
                "--unstable",
                "--filter",
                "fileReaderWriter.readFileContents"
            ],
            "outputCapture": "std",
            "attachSimplePort": 9229
        }
   ]
}